{
  "name": "tomasz-b-finelf-form-components",
  "version": "1.9.7",
  "main": "dist/js/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/js/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blueappsgroup/finelf-form-components.git"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-info": "^5.3.21",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.9",
    "@types/react-rangeslider": "^2.2.3",
    "@types/styled-components": "^5.1.4",
    "cross-env": "^7.0.2",
    "formik": "^2.2.1",
    "husky": "^4.3.0",
    "init": "^0.1.2",
    "lint-staged": "^10.5.1",
    "package.json": "^2.0.1",
    "react-datepicker": "^3.3.0",
    "react-rangeslider": "^2.2.0",
    "sb": "^6.0.28",
    "styled-components": "^5.2.1",
    "typescript": "~4.0.5"
  },
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build:lib": "yarn build:babel && yarn build:types && node ./scripts/copyTS.js",
    "build:babel": "concurrently \"yarn build:babel:esm && yarn build:babel:umd\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "BABEL_ENV=cjs babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js --presets=@babel/env",
    "build:babel:esm": "BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/esm",
    "build:babel:umd": "BABEL_ENV=umd babel --source-maps --extensions \".js\" dist/esm --out-dir dist/umd --plugins=transform-es2015-modules-umd",
    "build:types": "tsc -p tsconfig.gen-dts.json",
    "clean": "rimraf dist",
    "develop": "yarn build:types && yarn build:babel:esm --skip-initial-build --watch --verbose",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "react-scripts test --passWithNoTests",
    "test:watch-coverage": "react-scripts test --coverage --u",
    "test:coverage-all": "react-scripts test --watchAll=false --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.28",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.1",
    "concurrently": "^5.3.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/index.ts",
      "!src/type.ts",
      "!src/react-app-env.d.ts"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
